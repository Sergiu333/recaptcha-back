{
  "version": 2,
  "builds": [
    {
      "src": "index.js",  // Aici trebuie să pui fișierul principal (în cazul tău, index.js sau server.js)
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"  // Rotează toate cererile către serverul Express
    }
  ]
}
